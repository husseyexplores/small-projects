{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:css && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:css": "postcss src/assets/css/tailwind.css -o public/build/tailwind.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o public/build/tailwind.css --watch"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "core-js": "^2.6.5",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.0.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.1.4",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
